#include <stdio.h>


main(){
    int X[10] = {};
    int i, j, k;
    for (j = 0; j < 10; j++){
        scanf("%d", &X[j]);
    }
    for (i = 0; i < 10; i++){
        if (X[i] <= 0){
            X[i] = 1;
        }
    }
    for(k = 0; k < 10; k++){
        printf("X[%d] = %d\n", k, X[k]);
    }
	return 0;
}
